							$(".sub-head .slide_down > a")
									.click(
											function(e) {
												e.preventDefault();

												$(this).closest('.nav').find(
														'.slide_down').not(
														$(this).parent())
														.removeClass('active');

												if ($(this).parent().hasClass(
														'active') == false) {
													$(this).parent().addClass(
															'active');
												} else {
													$(this).parent()
															.removeClass(
																	'active');
												}

												$('html')
														.click(
																function(e) {
																	if ($(
																			e.target)
																			.parents(
																					'.slide_down').length < 1) {
																		//console.log('팝업 외 부분이 맞습니다')
																		$(
																				'.slide_down')
																				.removeClass(
																						'active');
																	}
																});
											});
						});
	</script>

